{
  "env": {
   "node": true
  },
  "rules": {
    "no-console": 2,
    "no-underscore-dangle": 0,
    "no-use-before-define": [2, "nofunc"],
    "max-len": [2, 150, 2],
    "quotes": [2, "single"],
    "keyword-spacing": 2,
    "no-multi-str": 0
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  }
}
